
<div class="container">
  <h1>Beers</h1>
  <div class="row">
      {{#each beersFromApi}} 
        
        <div class="card" style="width: 18rem;">
            <img src="{{this.image_url}}" class="card-img-top" alt="Beer image">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.description}}</p>
                <p class="card-text">{{this.tagline}}</p>
            </div>
        </div>

      {{/each}}
  </div>
</div>






